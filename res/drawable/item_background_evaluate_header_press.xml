<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="0.5dp" >
        <shape android:shape="rectangle">
            <solid android:color="@color/black" />
        </shape>
    </item>
    <item
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/evaluater_background" />
        </shape>
    </item>
    <item
        android:top="3.5dp"
        android:right="4.5dp"
        android:left="4.5dp"
        android:bottom="3.5dp" >
        <shape android:shape="rectangle">
            <solid android:color="@color/black" />
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item 
        android:top="4dp"
        android:right="5dp"
        android:left="5dp"
        android:bottom="4dp" >
        <shape android:shape="rectangle">
            <solid android:color="@color/about_us_seperate" />
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item 
        android:top="5dp"
        android:right="6dp"
        android:left="6dp"
        android:bottom="5dp" >
        <shape android:shape="rectangle">
            <solid android:color="@color/main_color_yellow" />
            <corners android:radius="2dp"/>
        </shape>
    </item>
</layer-list>